git_status() {\
  if git rev-parse --is-inside-work-tree > /dev/null 2>&1; then\
    local branch_name\
    branch_name=$(git rev-parse --abbrev-ref HEAD 2>/dev/null)\
    local status\
    status=$(git status --porcelain 2>/dev/null)\
\
    if [ -n "$status" ]; then\
      echo " [$branch_name*]"\
    else\
      echo " [$branch_name]"\
    fi\
  fi\
}
GIT_STATUS() {\
  if git rev-parse --is-inside-work-tree > /dev/null 2>&1; then\
    local branch_name\
    branch_name=$(git rev-parse --abbrev-ref HEAD 2>/dev/null)\
    local status\
    status=$(git status --porcelain 2>/dev/null)\
\
    if [ -n "$status" ]; then\
      echo " [$branch_name*]"\
    else\
      echo " [$branch_name]"\
    fi\
  fi\
}
\
  if git rev-parse --is-inside-work-tree > /dev/null 2>&1; then\
    local branch_name\
    branch_name=$(git rev-parse --abbrev-ref HEAD 2>/dev/null)\
    local status\
    status=$(git status --porcelain 2>/dev/null)\
\
    if [ -n "$status" ]; then\
      echo " [$branch_name*]"\
    else\
      echo " [$branch_name]"\
    fi\
  fi\
}
\
  if git rev-parse --is-inside-work-tree > /dev/null 2>&1; then\
    local branch_name\
    branch_name=$(git rev-parse --abbrev-ref HEAD 2>/dev/null)\
    local status\
    status=$(git status --porcelain 2>/dev/null)\
\
    if [ -n "$status" ]; then\
      echo " [$branch_name*]"\
    else\
      echo " [$branch_name]"\
    fi\
  fi\

$(git_status)
cd .scripts
exit
