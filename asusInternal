f [[ "$*" == "-c" ]]; then
sudo ip link set Internal down
sudo ip link set Internal down
sudo macchanger --mac=48:5B:39:E8:4D:B2 Internal
sudo ip link set Internal up

elif [[ "$*" == "-r" ]]; then
sudo ip link set Internal down
sudo ip link set Internal down
sudo macchanger -r Internal
sudo ip link set Internal up
fi

