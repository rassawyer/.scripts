## make sure the ssh agent is running
eval 'ssh-sgent'

## Add ssh keys to allow automatic login
ssh-add /home/rassawyer.ssh/github

## cd to the correct directory, using absolute paths
cd /home/rassawyer/.scripts

## add any untracked files, and any changes
## commit the new and modified files.
git commit -am 'updated package lists'

## push the changes to github
git push
